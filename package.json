    "clean": "rm -R ./dist"
  },
  "devDependencies": {
    "antd": "^3.26.4",
    "less": "^3.10.3",
    "less-plugin-clean-css": "^1.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
